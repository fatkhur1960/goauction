development:
    dialect: postgres
    datasource: host=${DB_HOST} dbname=${DB_NAME} user=${DB_USER} password=${DB_PASSWORD} sslmode=${SSL_MODE}
    dir: migrations/postgres
    table: migrations

test:
    dialect: postgres
    datasource: host=${DB_HOST} dbname=${DB_NAME_TEST} user=${DB_USER} password=${DB_PASSWORD} sslmode=${SSL_MODE}
    dir: migrations/postgres
    table: migrations